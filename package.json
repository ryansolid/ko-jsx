{
  "name": "ko-jsx",
  "description": "An alternative JSX renderer for Knockout.js",
  "version": "0.16.1",
  "author": "Ryan Carniato",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryansolid/ko-jsx"
  },
  "module": "dist/index.js",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "build": "rollup -c && ncp ./src/client.d.ts ./types/client.d.ts && ncp ./src/jsx.d.ts ./types/jsx.d.ts && tsc",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-typescript": "7.10.4",
    "@rollup/plugin-babel": "5.2.1",
    "@rollup/plugin-commonjs": "17.1.0",
    "@rollup/plugin-node-resolve": "11.2.0",
    "dom-expressions": "0.25.15",
    "hyper-dom-expressions": "0.25.15",
    "knockout": "3.5.1",
    "lit-dom-expressions": "0.25.15",
    "ncp": "2.0.0",
    "rollup": "^2.41.4",
    "typescript": "4.1.3"
  },
  "peerDependencies": {
    "knockout": "3.x"
  }
}
