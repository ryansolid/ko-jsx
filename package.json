    "lit-dom-expressions": "~0.15.0",
    "ncp": "2.0.0",
    "knockout": "3.5.1",
    "rollup": "^1.29.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-node-resolve": "5.2.0",
    "typescript": "3.7.4"
  },
  "peerDependencies": {
    "knockout": "3.x"
  }
